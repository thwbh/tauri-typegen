{
  "name": "tauri-typegen-example",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "generate-types": "../../target/debug/cargo-tauri-typegen tauri-typegen generate --project-path ./src-tauri --output-path ./src/generated --validation zod --verbose",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "npm run generate-types && tauri dev",
    "tauri:build": "npm run generate-types && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.2.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.2",
    "@tauri-apps/cli": "^2.2.0",
    "svelte": "^5.17.0",
    "vite": "^6.0.3"
  }
}
